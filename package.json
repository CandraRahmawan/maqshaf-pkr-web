{
  "name": "maqshaf-pkr-web",
  "version": "1.0.0",
  "description": "Frontend Web with Progressive Web Apps",
  "homepage": "https://github.com/CandraRahmawan/maqshaf-pkr-web#readme",
  "license": "https://pesantrenkhoirurrooziqiin.com",
  "author": "Candra Rahmawan <candraa.rahmawan@gmail.com>",
  "main": "./src/index.jsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/CandraRahmawan/maqshaf-pkr-web"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:watch": "react-scripts test",
    "test": "react-scripts test --watchAll=false --coverage"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "i18next": "^20.3.2",
    "i18next-http-backend": "^1.2.6",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^2.2.0",
    "react-query": "^3.18.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.3",
    "workbox-broadcast-update": "^5.1.3",
    "workbox-cacheable-response": "^5.1.3",
    "workbox-core": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-google-analytics": "^5.1.3",
    "workbox-navigation-preload": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-range-requests": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-streams": "^5.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "4.3.8",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "sass": "^1.35.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0"
  },
  "engines": {
    "node": ">=12.16.x",
    "yarn": ">=1.22.x"
  },
  "private": true,
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.js?(x)": "eslint --fix",
    "*.{js,jsx,json}": "prettier --write",
    "src/**/*.{js,jsx,scss,css}": "stylelint --fix",
    "public/**/*.{css,scss}": "stylelint --fix"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 5,
        "lines": 3,
        "statements": 3
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "syntax": "scss",
    "rules": {
      "at-rule-no-unknown": null,
      "no-descending-specificity": null,
      "font-family-no-missing-generic-family-keyword": null,
      "block-no-empty": null,
      "selector-type-no-unknown": null
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
